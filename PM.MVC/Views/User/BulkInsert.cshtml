@model PM.Model.Model.File
@{
    ViewData["Title"] = "BulkInsert";
}

<div class="container">
    <button class="btn btn-default">Download Format</button>
    <form asp-action="BulkInsert" method="post" enctype="multipart/form-data">
        <div class="row">
            <div class="col-md-3">
                <label>Select File For Bulk Insert</label>
                <input type="file" asp-for="FormFile" class="form-control" />
            </div>
        </div>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </form>
</div>

<h2>BulkInsert</h2>

